{
  "name": "rrouter",
  "version": "1.0.1",
  "description": "Testing react router",
  "main": "index.js",
  "homepage": "https://github.com/mandolyte/rrouter",
  "scripts": {
    "electron:start": "electron ./",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "rrouter",
    "linux": {
      "target": "deb",
      "icon": "app/android-chrome-512x513.png",
      "maintainer": "com.github.mandolyte"
    },
    "win": {
      "target": "nsis",
      "icon": "app/android-chrome-512x513.png"
    },
    "mac": {
      "category": "public.app-category.utilities",
      "target": "default",
      "icon": "app/android-chrome-512x513.png"
    }
  },
  "dependencies": {
    "@capacitor/electron": "^1.5.1",
    "electron-is-dev": "^1.1.0"
  },
  "devDependencies": {
    "electron": "^8.1.1",
    "electron-builder": "^22.4.1"
  },
  "keywords": [
    "capacitor",
    "electron"
  ],
  "author": {
    "name": "cecil.new",
    "email": "cecil.new@gmail.com"
  },
  "license": "MIT"
}
